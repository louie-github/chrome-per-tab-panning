{
  "manifest_version": 2,
  "name": "Per-tab Panning",
  "version": "1.0.0",
  "description": "Pan each tab separately left and right",
  "permissions": ["activeTab", "tabCapture"],
  "browser_action": {
    "default_icon": "icons/logo_512.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "512": "icons/logo_512.png"
  }
}
