{
  "name": "sip-per-tab-panning",
  "version": "1.0.0",
  "description": "A Chrome extension to allow pan each tab's audio left and right",
  "repository": "https://github.com/louie-github/sip-per-tab-panning",
  "author": "Louie Torres",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:scss-types": "tsm src/exports.module.scss",
    "build": "yarn run build:scss-types && parcel build src/popup.html src/background.ts",
    "watch": "yarn run build:scss-types && parcel watch src/popup.html src/background.ts"
  },
  "devDependencies": {
    "@parcel/config-webextension": "^2.2.1",
    "@parcel/transformer-sass": "^2.2.1",
    "@types/chrome": "^0.0.176",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "parcel": "^2.2.1",
    "parcel-reporter-static-files-copy": "^1.3.4",
    "parcel-reporter-web-ext": "^0.3.2",
    "typed-scss-modules": "^4.1.2",
    "typescript": "^4.5.4",
    "web-ext": "^6.6.0"
  },
  "dependencies": {
    "chrome-extension-async": "^3.4.1"
  },
  "packageManager": "yarn@3.1.1"
}
